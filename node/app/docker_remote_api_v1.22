  ]
  &quot;Processes&quot; : [
    [
      &quot;root&quot;,&quot;13642&quot;,&quot;0.0&quot;,&quot;0.1&quot;,&quot;18172&quot;,&quot;3184&quot;,&quot;pts/0&quot;,&quot;Ss&quot;,&quot;17:03&quot;,&quot;0:00&quot;,&quot;/bin/bash&quot;
    ],
    [
